{
    "name": "lazy-meal",
    "version": "1.0.0",
    "description": "Lunch/Dinner Selector Assistant",
    "private": true,
    "scripts": {
      "frontend:dev": "yarn --cwd ./frontend dev",
      "frontend:build": "yarn --cwd ./frontend build",
      "backend:dev": "yarn --cwd ./backend dev",
      "backend:start": "yarn --cwd ./backend start",
      "dev": "yarn frontend:dev & yarn backend:dev",
      "install:all": "yarn --cwd ./frontend && yarn --cwd ./backend"
    }
}